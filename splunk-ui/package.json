{
  "name": "splunk-ui",
  "version": "0.9.4",
  "publishConfig": {
    "registry": "https://repo.splunk.com/artifactory/api/npm/npm-solutions-local"
  },
  "license": "UNLICENSED",
  "scripts": {
    "build": "yarn run generate:licenses && NODE_ENV=production webpack --bail",
    "clean": "rimraf util && rimraf components && rimraf icons",
    "eslint": "eslint src --ext \".js,.jsx\"",
    "eslint:fix": "eslint src --ext \".js, .jsx\" --fix",
    "eslint:ci": "yarn run eslint -- -f junit -o test-reports/lint-results.xml",
    "generate:licenses": "splunk-node-license-extractor --json-direct-nondev-only",
    "lint": "yarn run eslint && yarn run stylelint",
    "lint:ci": "yarn run eslint:ci && yarn run stylelint",
    "start": "yarn run generate:licenses && NODE_ENV=development webpack --watch",
    "stylelint": "stylelint \"src/**/*.css\" --config stylelint.config.js",
    "test": "splunk-ui-test-runner start karma.conf.js --single-run",
    "test:ci": "yarn run test -- --reporters mocha,junit --junitDirectory test-reports --junitFilename unit-results.xml --no-colors",
    "test:watch": "splunk-ui-test-runner start karma.conf.js",
    "test:functional": "splunk-ui-functional-test-runner functional.local.conf.js --runnerConfig functional.runner.conf.js",
    "test:functional:ci": "splunk-ui-functional-test-runner functional.ci.conf.js --runnerConfig functional.runner.conf.js --logLevel verbose",
    "test:functional:cloud": "splunk-ui-functional-test-runner functional.cloud.conf.js --runnerConfig functional.runner.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@git.splunk.com:7999/ui/splunk-ui.git"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "dependencies": {
    "commonmark": "^0.27.0",
    "commonmark-react-renderer": "^4.3.2",
    "keycode": "^2.1.4",
    "lodash": "^4.14.2",
    "moment": "^2.18.1",
    "prismjs": "^1.6.0",
    "prop-types": "^15.5.8",
    "react-event-listener": "^0.4.3",
    "react-motion": "^0.4.8",
    "react-sortable-hoc": "^0.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-preset-splunk": "^0.3.0",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "enzyme": "^2.8.1",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-config-splunk": "^0.2.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "exports-loader": "^0.6.3",
    "fs-readdir-recursive": "^1.0.0",
    "imports-loader": "^0.7.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.18.1",
    "prompt": "^1.0.0",
    "raw-loader": "^0.5.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.5.4",
    "sinon": "^2.1.0",
    "splunk-node-license-extractor": "^1.1.2",
    "splunk-postcss-loader": "^0.2.0",
    "splunk-ui-functional-test-runner": "^1.0.3",
    "splunk-ui-test-runner": "^0.9.4",
    "splunk-webpack-tools": "^2.0.4",
    "static-server": "^2.0.3",
    "stylelint": "^7.9.0",
    "stylelint-config-splunk": "^1.0.1",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0"
  }
}
