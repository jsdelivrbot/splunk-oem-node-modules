{
    "name": "splunk-charting",
    "version": "0.4.4",
    "description": "Splunk library for building interactive charts",
    "license": "UNLICENSED",
    "main": "lib/index.js",
    "files": [
        "lib"
    ],
    "publishConfig": {
        "registry": "https://repo.splunk.com/artifactory/api/npm/npm-solutions-local"
    },
    "scripts": {
        "precommit": "lint-staged",
        "build": "webpack --config ./tools/webpack.build.config.js",
        "build:prod": "NODE_ENV=production yarn run build",
        "build:watch": "webpack --config ./tools/webpack.build.config.js --progress --watch",
        "ci": "IS_CI=true concurrently \"yarn lint:ci\" \"yarn test:unit:ci\" \"yarn test:integration:ci\" \"yarn run build\" \"yarn run build:playground\" -p name -n lint,test:unit,test:integration,build,playground",
        "ci-publish": "echo \"Nothing to publish yet\"",
        "deploy": "yarn run build:prod && np --tag latest",
        "format:js": "prettier --print-width 110 --tab-width 4 --single-quote --trailing-comma es5 --write",
        "format:examples": "node tools/format_examples",
        "format": "yarn format:js -- 'playground/src/**/*.{es,jsx}' 'test/visual/**/*.{js,jsx}' 'test/unit/**/*.js' 'src/**/*.{es,jsx}' && yarn format:examples",
        "lint:js": "eslint --ext .js,.es,.jsx src/ test/ playground/src",
        "lint:examples": "node tools/format_examples --lint",
        "lint": "yarn lint:js && yarn lint:examples",
        "lint:ci": "yarn lint:js -- -f junit -o test-reports/lint-results.xml",
        "start": "NODE_ENV=development webpack-dev-server --inline --hot --progress --config playground/webpack.config.js",
        "build:playground": "webpack --config playground/webpack.config.js",
        "test": "yarn lint && yarn test:unit && yarn test:integration && yarn test:visual",
        "test:unit": "NODE_ENV=test jest",
        "test:unit:watch": "NODE_ENV=test jest --watchAll",
        "test:unit:coverage": "NODE_ENV=test jest --coverage",
        "test:unit:ci": "yarn test:unit -- --ci",
        "test:integration": "karma start karma.conf.js --single-run",
        "test:integration:ci": "yarn test:integration -- --reporters mocha,junit --junitDirectory test-reports --junitFilename integration-results.xml --no-colors",
        "test:integration:just": "karma start karma.conf.js --reporters mocha --filter",
        "test:integration:watch": "karma start karma.conf.js",
        "test:integration:chrome": "karma start karma.conf.js --browsers=Chrome",
        "test:visual": "node tools/visualtest",
        "test:visual:ci": "node tools/visualtest -- --report-junit",
        "test:visual:local": "node tools/visualtest --local-selenium",
        "test:visual:clean": "node tools/visualtriage.js clean",
        "test:visual:clean:all": "node tools/visualtriage.js clean --include-untracked",
        "test:visual:triage": "node tools/visualtriage.js triage"
    },
    "dependencies": {
        "bowser": "^1.7.0",
        "highcharts": "5.0.12",
        "jquery": "2.1.0",
        "moment": "^2.18.1",
        "ui-splunk-web-core": "^0.0.17",
        "underscore": "1.6.0"
    },
    "devDependencies": {
        "@splunk/css-loader": "^0.3.1",
        "@splunk/eslint-config": "^0.3.0",
        "@splunk/karma-unit-test-runner": "^0.10.0",
        "@splunk/react-icons": "^0.2.0",
        "@splunk/react-ui": "^0.10.3",
        "@splunk/wdio-functional-test-runner": "pr846",
        "@splunk/webpack-configs": "^0.2.0",
        "babel-eslint": "^7.2.3",
        "babel-loader": "^6.3.2",
        "babel-plugin-transform-amd-to-commonjs": "^0.2.2",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.23.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.23.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "chai": "^4.0.0",
        "chalk": "^2.0.1",
        "concurrently": "^3.4.0",
        "eslint": "^3.19.0",
        "eslint-config-airbnb": "14.1.0",
        "eslint-config-airbnb-base": "^11.2.0",
        "eslint-plugin-import": "2.2.0",
        "eslint-plugin-jsx-a11y": "4.0.0",
        "eslint-plugin-react": "6.9.0",
        "execa": "^0.7.0",
        "html-webpack-plugin": "^2.29.0",
        "husky": "^0.14.3",
        "imports-loader": "^0.7.1",
        "inquirer": "^3.1.1",
        "jest-cli": "^20.0.4",
        "jest-junit": "^1.5.1",
        "karma": "^1.7.0",
        "karma-chrome-launcher": "^2.1.1",
        "karma-junit-reporter": "^1.2.0",
        "karma-mocha": "^1.3.0",
        "karma-mocha-reporter": "^2.2.3",
        "karma-phantomjs-launcher": "^1.0.4",
        "lint-staged": "^4.0.2",
        "minimatch": "^3.0.4",
        "mocha": "^3.4.2",
        "np": "^2.15.0",
        "prettier": "^1.5.3",
        "prop-types": "^15.5.10",
        "react": "^15.6.1",
        "react-ace": "^5.0.1",
        "react-dom": "^15.6.1",
        "react-redux": "^5.0.5",
        "react-resizable": "^1.7.1",
        "redux-saga": "^0.15.4",
        "reselect": "^3.0.1",
        "rimraf": "^2.6.1",
        "sinon": "1.17.6",
        "style-loader": "^0.18.2",
        "styled-components": "^2.1.0",
        "svg-inline-loader": "^0.7.1",
        "term-img": "^1.0.0",
        "wdio-visual-regression-service": "^0.8.0",
        "webpack": "^2.6.0",
        "webpack-dev-server": "^2.5.0",
        "webpack-merge": "^4.1.0",
        "yargs": "^8.0.2"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.js"
        ],
        "coverageReporters": [
            "json",
            "lcov",
            "text",
            "html"
        ],
        "moduleDirectories": [
            "node_modules",
            "src"
        ],
        "testRegex": "test/unit/.*\\.test\\.js$",
        "transform": {
            "^.+\\.(jsx?|es)$": "babel-jest"
        },
        "testResultsProcessor": "./tools/jestresultsprocessor"
    },
    "lint-staged": {
        "playground/examples/**/*.json": [
            "format:examples",
            "git add"
        ],
        "playground/src/**/*.{js,es,jsx}": [
            "format:js",
            "git add"
        ],
        "src/**/*.{js,es,jsx}": "lint:js",
        "test/**/*.{js,es,jsx}": "lint:js"
    }
}
