{
  "name": "splunk-ui",
  "version": "0.6.2",
  "publishConfig": {
    "registry": "https://repo.splunk.com/artifactory/api/npm/npm-solutions-local"
  },
  "scripts": {
    "build": "yarn run build:docs && yarn run build:src",
    "build:src": "yarn run clean:src && NODE_ENV=production node tools/run bundle webpack.config.js",
    "build:docs": "yarn run clean:docs && NODE_ENV=production node tools/run bundle docs/webpack.config.js",
    "clean": "yarn run clean:docs && yarn run clean:src",
    "clean:docs": "rimraf docs/build",
    "clean:src": "rimraf util && rimraf components && rimraf icons",
    "create:component": "node tools/run createComponent",
    "deploy": "cd docs/build && static-server -p 8012 -n index.html",
    "eslint": "eslint src tools docs/src --ext \".js,.jsx\"",
    "eslint:fix": "eslint src tools docs/src --ext \".js, .jsx\" --fix",
    "lint": "yarn run eslint && yarn run stylelint",
    "start": "cd docs && webpack-dev-server --hot --inline --history-api-fallback --host 0.0.0.0",
    "stylelint": "stylelint \"src/**/*.css\" \"docs/src/**/*.css\" --config stylelint.config.js",
    "test": "karma start karma.conf.js --single-run",
    "test:watch": "karma start karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@git.splunk.com:7999/ui/splunk-ui.git"
  },
  "peerDependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "dependencies": {
    "commonmark": "^0.27.0",
    "commonmark-react-renderer": "^4.3.2",
    "keycode": "^2.1.4",
    "lodash": "^4.14.2",
    "prismjs": "^1.6.0",
    "react-event-listener": "^0.4.0",
    "react-motion": "^0.4.5"
  },
