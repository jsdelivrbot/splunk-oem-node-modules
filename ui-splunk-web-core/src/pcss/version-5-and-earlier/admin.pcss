	padding:    3px;
}

#appsSplunkbaseApps #pager li.currentpage {
	border: 1px solid $borderColor;
}

/*********************
    Widget styles
**********************/

/* Accumulator Styles */
.accumulator fieldset {
    margin:0;
}
.accumulator select.hiddenSelectedOptions {
    display:none;
}
.accumulator .availableOptionsContainer,
.accumulator .selectedOptionsContainer {
    float: left;
    width:300px;
    margin: 0px 20px 0px 0px;
}
.accumulator .addAllLink,
.accumulator .removeAllLink {
    float:right;
}
.accumulator .selectedOptionsContainer {
    margin-right:0px;
}
.accumulator h4 {
    font-weight:normal;
    font-size: $baseFontSize;
    width:200px;
    margin-right:22px;
    padding-bottom:5px;
}
.accumulator h4.selectedOptionsHeader {
    width: 200px;
    margin-right:0px;
}
.accumulator ul.availableOptions,
.accumulator ul.selectedOptions {
    height:200px;
    overflow: auto;
    /*width:318px;
    float: left;*/
    list-style: none none;
    margin: 0;
    padding:0;
    border: 1px solid $borderColor;
    clear: left;
}
.accDisabled ul.availableOptions,
.accDisabled ul.selectedOptions {
    background-color: #f0f0f0;
}
.accumulator ul.availableOptions li,
.accumulator ul.selectedOptions li {
    clear:left;
    padding: 4px 5px;
    font-size: $baseFontSize;
    cursor: pointer;
    line-height: 12px;
}
.accumulator ul.availableOptions li:hover,
.accumulator ul.selectedOptions li:hover {
    background-color: #e5f2f5;
}
.accumulator ul.availableOptions li span,
.accumulator ul.selectedOptions li span {
    margin:0px 8px 0 0;
    display: block;
    float: left;
}
.accumulator ul span.splIcon-arrow-e {
    background-color:  $splunkGreen;
}
.accumulator ul.availableOptions li.selected,
.accDisabled ul li {
    color: $textGray;
}
.accumulator ul.availableOptions li.selected span.splIcon,
.accDisabled ul li span.splIcon {
    background-color:$grayLightMedium;
}

.radioWidget ul {
    list-style: none none;
    margin: 0;
    padding: 0;
}

.fieldmapping input {
    width: 150px;
}

/*********************
    Data inputs
**********************/

table#dataInputs td {
    border-bottom: 0;
}

table#dataInputs td.input_description {
    border-bottom: 1px solid $borderColor;
    padding-top: 0px;
}

td.input_name {
    font-size: 14px;
    font-weight: bold;
}
#eaiform.entityEditForm .disabledTextarea pre,
#eaiform.entityEditForm textarea {
    font-family: monospace, courier;
    font-size: $baseFontSize;
}

#eaiform h2 {
    margin:0px 10px 10px 10px;
    font-weight: bold;
    font-size: 18px;
    letter-spacing: -0.5pt;
    color: $splunkGreen
}

