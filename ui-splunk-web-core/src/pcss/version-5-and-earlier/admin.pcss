}

.jmFormActions .splButton-primary {
    float:right;
}

.admin-dialog-tray .splButton-secondary,
.jmFormActions .splButton-secondary {
    float:left;
}

.error-message {
	background-color: red;
	border: 1px $borderColor solid;
}

.notification {
	background-color: yellow;
	border: 1px $borderColor solid;
}

.msgBox {
    padding: 10px;
    background: #f2f2f2;
    margin: 20px 3%;
