

/* Styles for the admin container and list views
 *
 *    This file only included for the admin section when wrapped in full admin skin.  See adminforms.css for
 *    styles specific to form elements themselves (which would be included when wrapped in the admin skin or in popups).
 */

/* body */
body.splView-_admin {
	background: $bodyBackgroundColor;
}

.navigationHeader {
    background-color: transparent;
}

/* styling for about dialog */
.aboutPopupContainer {
    display:none;
    padding: 10px;
}
.aboutPopupContainer .aboutPopupSplunkLogo {
    background: transparent url(/static/img/skins/default/logo_splunkServer_white_low.png) no-repeat scroll 0 0;
    height: 45px;
    width: 120px;
}

.ManagerBar #managerContextSwitcherActivator{
    padding-right: 15px;
    position: relative;
}
.ManagerBar #managerContextSwitcherActivator .splIcon-triangle-3-s {
    position:absolute;
    top:9px;
    right:10px;
    background-position: -26px -327px;
    width:7px;
}

/* end header */


/* content */
.adminContent {
  clear: both;
  min-height: 400px;
}

/* index page */

/* index page */

div.adminIndex {
    overflow: hidden;
    font-size: $baseFontSize;
    padding-top:20px;
    padding-left:20px;
}
div.adminIndex div.adminSections {
    overflow:hidden;
    clear:left;
}
div.adminIndex div.adminIndexLeft, div.adminIndex div.adminIndexRight {
    float:left;
    width:50%;
    padding:0;
    margin:0;
}
div.adminIndex div.adminGroup {
    padding: 0px;
    border:1px solid $borderColor;
    box-shadow: 0 0 5px $grayLightMedium;
    border-radius:5px;
    margin-bottom: 20px;
    margin-right:20px;
    position: relative;
    background: $white;
}
div.adminIndex h2 {
    font-size: 18px;
    font-weight: bold;
    margin: 0;
    color: $splunkGreen;
    text-shadow: $white 0px 1px 1px;
    margin-bottom: 5px;
    padding-left: 35px;
}
div.adminIndex div.lastCell {
    border: none;
}
div.adminIndex h1 {
    margin:10px 0px 10px 10px;
    font-weight: bold;
    font-size: 21px;
    letter-spacing: -0.5pt;
}
div.adminIndex p {
    margin-top: 20px;
    padding: 0px 10px;
}
div.adminIndex dl {
    padding: 0;
    margin: 0;
}
div.adminIndex div.adminListItem {
    padding: 10px;
    background-repeat: no-repeat;
    background-position: 0 0;
    border-bottom: 1px dotted $borderColor;
}
div.adminIndex div.adminListItem:last-child {
	border-bottom: none;
}
div.adminIndex .adminListIcon {
    display: block;
    height: 36px;
    width: 36px;
    margin-right: 10px;
    margin-left: 10px;
    float: left;
    background-size: 36px;
}
div.adminIndex dl dt {
    margin: 0 0 5px 0;
    font-weight: normal;
    font-size: $baseFontSizeLarge;
}
div.adminIndex dl dt a {
    font-size: $baseFontSizeLarge;
    padding: 0 0 25px 24px;
    white-space: nowrap;
}
div.adminIndex dl dt span a {
    font-size: $baseFontSizeSmall;
}
div.adminIndex dl dd {
    margin:0 0 0 24px;
    color:$textGrayDark;
}

/* SPL-55408 - for IE*/
#adminIndexRadios .authCheckbox {
    border:none;
}
#adminIndexRadios input[type='checkbox']{
    padding:5px 7px 5px 7px;
}

/* end index page */

/* server restart */
#restarterr { color: red; font-weight: bold; display: none; }
#restartstatus, #restartsslwarn {
    display: none;
    position: fixed;
    top: 45px;
    left: 50%;
    z-index: 900; /*place behind the disconnect message */
    width: 400px;
    margin-left: -220px;
    background-color: $white;
    border-radius: 6px;
    box-shadow: 0 3px 7px rgba(0, 0, 0, 0.3);
    color: $grayDark;
    padding: 20px;
    text-align: center;

}
#restartsslwarn {
    top: 35px;
    z-index: 1100; /*place over the disconnect message */
    width: 400px;
    height: 150px;
    margin-left: -270px;
    padding: 20px 70px;
}

#restartstatus h1, #restartsslwarn h1 {
    font-size: $baseFontSizeLarge;
    font-weight: normal;
    color: $grayDark;
}
#restartstatus .sslwarn {
    display: none;
}
#restartstatus p, #restartsslwarn p {
  font-size: $baseFontSize;
}


/* end server restart */

/* server control page */

.servercontrol {
    width:600px;
    padding:10px 20px;
}
.servercontrol .splButton-primary {
    margin-left:0px;
    margin-top:10px;
}

/* loading message */
.editFormWrapper .splOverlay-white {
    position:absolute;
}
/* end loading message */

/* edit pages */
div.editFormWrapper {
    padding:0px;
    position:relative;
}
/* end edit pages */

/* data inputs */
div.dataInputsWrapper, div.pageContentWrapper {
    padding:20px;
    background: $white;
    border-top: 1px solid $borderColor;
    min-height: 420px;
}

div.dataInputsWrapper h1, div.pageContentWrapper h1 {
	color: $splunkGreen;
	font-size: 18px;
	font-weight: bold;
}

div.dataInputsWrapper .dataInputsDesc, div.pageContentWrapper .pageContentDesc {
	display: block;
	font-size: $baseFontSize;
	margin-top: 5px;
	margin-bottom: 15px;
}

div.dataInputsWrapper .fieldsetWrapper.first {
	padding-bottom: 15px;
	padding-top: 0px;
}

div.dataInputsWrapper table p.input_description {
	font-style: italic;
	margin: 0px;
	font-size: $baseFontSizeSmall;
	font-weight: normal;
	padding-top: 5px;
}

div.dataInputsWrapper .listWrapper {
	padding: 0;
}
/* end data inputs */

/* list pages */
.adminContentList {
	background: $white;
	border-top: 1px solid $borderColor;
	border-bottom: 1px solid $borderColor;
	margin-bottom: -1px;
}
.adminContentList h1 {
	font-weight: bold;
}
div.listWrapper {
    padding:20px;
}
div.actionButtons {
    overflow: hidden;
}
div.listWrapper table.splTable th,
div.listWrapper table.splTable td {
    padding-right: 20px;
}
div.listWrapper table.splTable .splPipe {
    margin:0px 2px;
}
table.splTable td input {
    margin:0px;
}
table.splTable {
    background-color:$white;
}
.listIntroText {
    padding: 10px;
}
/* end list pages */

/* filter controls */
.eaiFilterControls {
	padding: 10px 0px;
    border-bottom: 1px solid $borderColor;
    margin-left: 0;
}
.eaiFilterControls .eaiFilterControl {
	float: left;
	padding-top: 3px;
}
/** Firefox hack **/
@-moz-document url-prefix()
{
	.eaiFilterControls .eaiFilterControl {
		padding-top: 0px;
	}
}
.eaiFilterControls .eaiFilterControl.eaiSearchControl {
	margin-right: 20px;
	padding-top: 0px !important;
	float: right;
}
.eaiFilterControls input, .eaiFilterControls select {
    margin:0;
    border:1px solid $borderColor;
    width:auto;
    vertical-align: middle;
    background-color: $white;
    max-width: 180px;
}
.eaiFilterControls input.checkbox, .eaiFilterControls input.radio {
    width:auto;
    background-color: transparent;
    border: none;
}
.hasTabs .eaiFilterControls {
    background-color:$white;
    border-bottom:1px solid $borderColor;
    margin:0px 10px;
}
.eaiFilterControls input.eaiFilter {
     background: $white;
     width:250px;
     padding: 3px 5px 4px;
}
.eaiFilterControls label {
    margin: 0 5px 0 20px;
    vertical-align: middle;
    text-shadow: $white 0px 1px 1px;
}
.eaiFilterControls .appOnlyFilter input {
	margin-left: 15px;
}
.eaiFilterControls .appOnlyFilter label {
	margin-left: 0px;
	margin-right: 0px;
}
.appOnlyFilter {
	padding: 5px 20px;
	border-bottom: 1px solid $borderColor;
}
.appOnlyFilter input {
	margin: 0px;
}
/* end filter controls */

.eaiPagination {
    margin-bottom: 10px;
    padding: 15px 10px 0px;
}

.eaiPagination .eaiPaginationHeader h2 {
	float: left;
	margin-right: 10px;
	margin-top: 2px;
}

.eaiPagination div.actionButtons {
	float: left;
}

.eaiResultsPageControls {
	padding: 0px 20px 5px;
}
.eaiResultsPageControls .eaiResultsPageControl {
	float: left;
	margin-right: 15px;
	margin-top: 0px;
}
.eaiResultsPageControls .Paginator {
	margin-left: 15px;
	margin-top: -4px;
}
.eaiResultsPageControls .eaiResultsPerPage {
    float: right;
    margin-right: 0px;
    margin-top: -2px;
}
.eaiResultsPerPage select {
	margin: 0px;
}
.eaiResultsCount {
	float: left;
	font-size: $baseFontSize;
	margin: 0px;
}
.eaiResultsPerPage label {
    font-size: $baseFontSizeSmall;
    margin-right:5px;
}
.eaiResultsPerPage select {
    font-size: $baseFontSizeSmall;
    width:55px;
}
.eaiPagination h2 {
    font-weight:bold;
    font-size: 18px;
    color: $splunkGreen;
    margin:0px;
}
.eaiPagination p span {
    font-weight: bold;
}
.eaiPagination .Paginator {
    padding-top: 5px;
}
.eaiPagination select {
    margin-top: 0px;
}
.checkboxCell {
    width: 20px;
}


.bolt {
    width:auto;
    height:16px;
    padding-top: 3px;
    padding-left:9px;
    padding-right:2px;
}
.rssLink {
    width:12px;
    height:12px;
    background: url(/static/img/skins/default/xml.gif) no-repeat 0 50%;
    float:left;
}
.enabledCheck {
    width:6px;
    height:7px;
    background: url(/static/img/skins/default/check.gif) no-repeat 0 50%;
    float:left;
    margin-top:3px;
    margin-right:5px;
}

.enabledCheckNotPresentFiller {
    width:21px;
    height:7px;
    float:left;
}

/* styling for move dialog */
.moveObjectFormContainer {
    display:none;
    padding: 10px;
}

/* end list pages */

/* license page */

div.licenseWrapper {
    padding:10px;
}
div.licenseWrapper .splTable {
    width: 620px;
}
div.licenseWrapper .datarow th {
    border-bottom: 1px dotted #ddd;
    width:110px;
}
div.licenseWrapper td {
    width:200px;
}
div.licenseWrapper table.splTable tr:hover td {
    background-color:transparent;
}

/* end license page */

/* forward/receive page */

.forwardReceive ul {
    margin:5px 0 10px;
    padding:0;
}
.forwardReceive ul li {
    list-style: none none;
    padding: 3px 0px;
}
.forwardReceive .forwardReceiveHelp {
    margin:10px 0;
}

/* end forward/receive */

/* shared styles */

.adminTitleStyle label {
    color:$splunkGreen;
    font-size: 14px;
    font-weight: bold;
}

legend.hasHelpText {
    padding-bottom:10px;
}
div.standaloneHelp p.helpText {
    display:block;
    margin: 0 0 5px 0;
}

.splTable h1 {
    margin:0;
}

.entityEditForm {
