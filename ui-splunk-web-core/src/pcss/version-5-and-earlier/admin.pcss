    color:$textGrayDark;
}

/* override overrides for search button */
input.searchButton {
    margin: 7px 0 0 10px;
    width: 32px;
    border: 0px;
    outline:0;
    height: 24px;
    background-color: #659c40;
    background-position: -51px center;
}
input.searchButton:hover {
    background-position: -51px center;
}
.eaiFilterControls input.searchButton {
    margin-top:-2px;
}

/* end shared */

