/******** START CSS FOR GDI ********************************************/
/***********************************************************************/

.spl-mgr-advanced-switch {
	border-top:1px dotted $borderColor;
	padding:5px 0;
}
.spl-mgr-advanced-switch label {
	color:$splunkGreen;
	font-size:14px;
	font-weight:bold;
}
.spl-mgr-advanced-options {
	margin:10px;
	margin-top:0px;
	padding:0 20px 20px;

	border: $borderLightColor 1px solid;
	border-radius: 3px;
}




/*** General visual styles ***/
span.spl-hr-solid,
span.spl-hr-dotted-lite,
span.spl-hr-solid-lite {
	border-left: 0;
	border-right: 0;
	border-top: 0;
	display: block;
	font-size: 1%;
}
span.spl-hr-solid,
span.spl-hr-dotted-lite {
	padding: 0;
	margin: 10px 0;
}
span.spl-hr-solid {
	border-bottom: 1px solid $borderDarkColor;
}
span.spl-hr-dotted-lite {
	border-bottom: 1px dotted $borderColor;
}

span.spl-hr-solid-lite {
/*
	border-bottom: 1px solid $borderColor;
	padding: 0;
	margin: 10px 0 30px 0;
*/
	display: none;
}
.spl-gdi-wrapper .spl-border-box-sizing {
	box-sizing: border-box;
}

/*** End General visual styles ***/

/*** Data inputs pages styles ***/

.spl-gdi-wrapper {
	position: relative;
	height: auto;
	width: 660px;
	margin: 30px auto 20px auto;
	padding-top: 15px;
	box-sizing: border-box;
	background: $white;

	border: 1px solid $borderColor;
	border-radius: 2px;
}
.spl-gdi-wrapper .jmFormActions-btn-left {
	text-align: left;
}
.spl-gdi-wrapper .jmFormActions-btn-left .splButton-primary {
	float: left!important;
}

.spl-prompt-message-layout .jmFormActions {
	text-align: left!important;
}

.spl-prompt-message-layout a.splButton-primary span,
.spl-prompt-message-layout a.splButton-secondary span {
	line-height: 21px;
}

.spl-gdi-wrapper .jmFormActions .splButton-secondary span {
    min-width: 80px;
}

.spl-gdi-wrapper div.spl-clear-fix {
	clear:both;
	height:1px;
	margin-top:-1px;
	overflow:hidden;
	font-size: 1%;
}
.spl-gdi-wrapper * {
	line-height: 1;
}
.spl-gdi-wrapper * p {
	font-size: inherit;
	color: #111;
	padding: 0;
	margin: 0;
}
.spl-gdi-wrapper h2.spl-page-hdr {
	margin-top: 0;
	margin-bottom: 5px;
	font-size:18px;
	font-weight: bold;
	color:$splunkGreen;
	position: relative;
	padding: 0px 10px;
}
.spl-gdi-wrapper h2.spl-page-hdr a.spl-help-link {
   	position: absolute;
	right: 0;
	top: 0;
    display: inline-block;
    height: 16px;
    line-height: $baseLineHeight;
    padding-left: 20px;
    white-space: nowrap;
    font-size: $baseFontSize;
    text-align: right;
    font-weight: bold;
}
.spl-gdi-wrapper .spl-col-floating-link-left {
	font-size: $baseFontSize;
	margin-left: 10px;
}
.spl-gdi-wrapper .spl-col-floating-link-left span {
	margin-top: 20px;
	margin-bottom: 10px;
	display: inline-block;

}

.spl-gdi-wrapper .spl-col-floating-link-left .spl-icon-external-link-sm {
    padding-right: 2px;
}

.icon-external {
    padding-left: 4px;
    padding-right: 4px;
}

/* three cols */

.spl-gdi-wrapper .spl-three-col-wrapper {
	float:  left;
	width: 100%;
	padding: 0;
}
.spl-gdi-wrapper  .spl-three-col-wrapper-inner {
	padding: 0 10px;
	margin-top: 5px;
}
.spl-gdi-wrapper .spl-three-col-wrapper .spl-left-col,
.spl-gdi-wrapper .spl-three-col-wrapper .spl-mid-col,
.spl-gdi-wrapper .spl-three-col-wrapper .spl-right-col {
	display: inline;
	float: left;
	width: 30%;
}
.spl-gdi-wrapper .spl-three-col-wrapper .spl-mid-col {
	margin: 0px 15px;
}
.spl-gdi-wrapper .spl-col-border-left {
	border-left: 1px dotted $borderColor;
}
.spl-gdi-wrapper .spl-col-border-right {
	border-right: 1px dotted $borderColor;
}
.spl-gdi-wrapper ul.spl-data-options-links {
	min-height: 70px;
	list-style: none;
	margin: 0;
	padding: 0;
}
.spl-gdi-wrapper ul.spl-data-options-links li {
	padding:4px 0;
}
.spl-gdi-wrapper ul.spl-data-options-links li p {
	font-size: $baseFontSizeSmall !important;
	color:$textGrayDark !important;
	padding:0;
	margin: 3px 10px 10px 0;
}
.spl-gdi-wrapper ul.spl-data-options-links li a {
	font-size: $baseFontSize;
}
.spl-gdi-wrapper ul.spl-data-options-links-strong li a {
	font-weight: bold !important;
}
.spl-gdi-wrapper h3.spl-tan-section-head {
	padding: 10px;
	clear: both;
	border-bottom: 1px solid $borderColor;
	box-sizing: border-box;
	color:#111;
	font-size:13px;
	font-weight: normal;

}
.spl-gdi-wrapper h3.spl-tan-section-head-top {
	margin-top: 20px;
}

.spl-gdi-wrapper .spl-data-options-links li.spl-icon-local-event-log-sm,
.spl-gdi-wrapper .spl-data-options-links li.spl-icon-remote-event-log-sm,
.spl-gdi-wrapper .spl-data-options-links li.spl-icon-files-dirs-log-sm,
.spl-gdi-wrapper .spl-data-options-links li.spl-icon-wmi-collections-sm,
.spl-gdi-wrapper .spl-data-options-links li.spl-icon-tcp-sm,
.spl-gdi-wrapper .spl-data-options-links li.spl-icon-udp-sm,
.spl-gdi-wrapper .spl-data-options-links li.spl-icon-registry-monitoring-sm,
.spl-gdi-wrapper .spl-data-options-links li.spl-icon-active-dir-monitoring-sm,
.spl-gdi-wrapper .spl-data-options-links li.spl-icon-scripts-sm {
	padding: 2px 0px 10px 20px!important;
}

.adminContent .spl-gdi-wrapper h3.add-data-header {font-weight:bold;}

/*** End Data inputs pages styles ***/


/*** Recipe page styles ***/

.spl-gdi-wrapper .spl-recipes-wrapper {
	position: relative;
	display: block;
}
.spl-gdi-wrapper .spl-recipes-wrapper ul.spl-recipes-list {
	list-style: none;
	margin: 0;
	padding: 0;
}
.spl-gdi-wrapper .spl-recipes-wrapper ul.spl-recipes-list li.spl-recipe-list-item {
	padding: 0 10px 15px 45px;
	margin-bottom: 15px;
	border-bottom: 1px dotted $borderColor;
}
.spl-gdi-wrapper .spl-recipes-wrapper ul.spl-recipes-list li.spl-recipe-last {
	border-bottom: 0px dotted $borderColor!important;
}
.spl-gdi-wrapper .spl-recipes-wrapper ul.spl-recipes-list li p.spl-recipe-description {
	margin: 0 0 10px 0;
	color:$textGrayDark;
	font-size: $baseFontSizeSmall;
	line-height: 15px;
}
.spl-gdi-wrapper p.spl-main-description a.spl-inline-link,
.spl-gdi-wrapper .spl-recipes-wrapper ul.spl-recipes-list li p.spl-recipe-description a.spl-inline-link {
	background-position:0 0px;
	margin-left:3px;
	padding-left:15px;
	font-size: $baseFontSizeSmall;
	line-height:13px;
}
.spl-gdi-wrapper .spl-recipes-wrapper ul.spl-recipes-list li span.spl-recipe-action {
	display:inline-block;
	height:16px;
}
.spl-gdi-wrapper .spl-recipes-wrapper ul.spl-recipes-list li span.spl-recipe-action a.spl-action-link {
	display:block;
	background-position:0 0;
	font-size: $baseFontSize;
	line-height: $baseLineHeight;
	padding:0 0 0 20px;
}
.spl-gdi-wrapper .spl-recipes-wrapper ul.spl-recipes-list li h3.spl-recipe-hdr {
	font-size: 14px;
	font-weight: 300;
	color: #111;
	margin-bottom: 3px;
}
.spl-gdi-wrapper h3.spl-main-description-hdr {
	font-size: $baseFontSizeLarge;
	font-weight:300;
	color:$splunkGreen;
	position: relative;
	margin: 5px 0 10px 0;
}
.spl-gdi-wrapper p.spl-main-description {
	font-size: $baseFontSize;
	font-weight:300;
	line-height:140%;
	padding-bottom:0px;
	margin-top: 0;
	text-align: justify;
}

/*** End Recipe page styles ***/

/*** Start success page styles ***/

.spl-success-message-page {
	text-align: center;
}
.spl-gdi-success-layout-wrapper {
	position: relative;
	text-align: center;
	margin-top: 10px;
}
.spl-gdi-wrapper span.spl-success-message-wrapper {
	display: inline-block;
	padding: 40px 40px 30px 40px;
}
.spl-gdi-wrapper .spl-success-message-wrapper-inner {
	position: relative;
	text-align: center;
}
.spl-gdi-wrapper span.spl-success-message-content {
	position: relative;
	display: inline-block;
	text-align: left;
	padding-left: 60px;
	background-position: 0 0;
}
.spl-gdi-wrapper h1.spl-success-message {
	box-sizing:border-box;
	padding:0;
	margin: 0 0 5px 0;
	font-size:24px;
	font-weight:300;
	text-align: left;
	letter-spacing:-0.3pt;
	white-space: normal;
}
.spl-gdi-wrapper span.spl-success-sub-message,
.spl-gdi-wrapper span.spl-success-sub-message span.spl-success-sub-message-inner {
	display:inline-block;
	position:relative;
	box-sizing: border-box;
}
.spl-gdi-wrapper span.spl-success-sub-message {
	text-align: left;
	border-radius:$interactiveBorderRadius;
}
.spl-gdi-wrapper span.spl-success-sub-message span.spl-success-sub-message-inner {
	text-align: center;
	background-color:$yellowLighter;
	padding:5px 10px;
}
.spl-gdi-wrapper .spl-success-actions-wrapper {
	position: relative;
	display: block;
	margin: 20px auto 0 auto;
}
.spl-gdi-wrapper .spl-success-actions-wrapper-inner {
	padding:0;
	text-align: left;
}
.spl-gdi-wrapper .spl-success-actions-list {
	margin: 0;
}
.spl-gdi-wrapper .spl-success-actions-item {
	border-bottom: 1px dotted $borderColor;
	display: block;
}
.spl-gdi-wrapper .spl-success-actions-item-last {
	border: 0 solid;
}
.spl-gdi-wrapper .spl-success-actions-link {
	display: block;
	height: 48px;
	font-size: $baseFontSizeLarge;
	line-height: 48px;
	padding-left: 45px;
	background-position: 0 50%;
}
/*** End success page styles ***/


/*** Prompt message styles ***/

.spl-prompt-message-layout h2.spl-page-hdr {
	position:relative;
	height:24px;
	color:$splunkGreen;
	font-size:18px;
	font-weight:bold;
	line-height:24px;
	margin-bottom:10px;
	margin-top:0;
	padding-left:30px;
}
.spl-prompt-message-layout h2.spl-no-icon-no-padding {
	padding-left: 10px;
}
.spl-gdi-wrapper .spl-prompt-message-wrapper {
	position: relative;
	width: inherit;
	text-align: left;
}
.spl-gdi-wrapper .spl-prompt-message-wrapper .error {
	color:$white;
	display:block;
	margin-bottom:10px;
	padding: 5px 0;
	text-indent:10px;
	width:300px;
}
.spl-gdi-wrapper .spl-prompt-message-wrapper .adminFormItem {
	margin-bottom: 10px;
}
.spl-gdi-wrapper .spl-prompt-message-wrapper .adminFormItem label {
	display: block;
}
.spl-gdi-wrapper .spl-prompt-message-wrapper .spl-main-description {
	margin-bottom: 15px;
}
.spl-gdi-wrapper .spl-prompt-message-wrapper .spl-main-description strong {
	color: $splunkGreen;
	font-weight: bold;
}
.spl-gdi-wrapper .spl-prompt-message-wrapper .spl-license-warn {
    color: $errorText;
    display: none;
    margin-left: 20px;
    margin-top: -10px;
    margin-bottom: 10px;
}

/*** end prompt message styles ***/

