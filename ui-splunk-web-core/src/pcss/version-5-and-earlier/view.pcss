.reportBuilder .EnablePreview{float:left;line-height:22px;}
.reportBuilder .AddTotals{float:left;line-height:22px;}


/*508 helper class - Hide only visually, but have it available for screenreaders: h5bp.com/v */
.visuallyhidden { border: 0; clip: rect(0 0 0 0); height: 1px; margin: -1px; overflow: hidden; padding: 0; position: absolute; width: 1px; }

