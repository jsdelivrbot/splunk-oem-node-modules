.view {
	font-size: $baseFontSize;

	p {
	  margin: 0 0 calc($baseLineHeight / 2);
	}
}

.dotted {
    line-height: 20px;
    margin-top: 0;
    dt {
        float: left;
        width: 120px;
        overflow: hidden;
        white-space: nowrap;
        margin-right: 5px;
        font-weight: normal;
        line-height: 20px;
        /*  SPL-69719 - fix IE10 when in dropdown */
        word-wrap: normal;
    }
    dd {
        line-height: 20px;
        margin-left: 125px;
    }
    dt:after {
        content: " ............................................";
    }
}

.divider {
	@mixin nav-divider;
	margin-top: 15px;
}


.appSectionHeader {
	margin-top: 10px;
	font-weight: bold;
}

.attributionLink {
	margin-bottom: 10px;
	display: block;
}
