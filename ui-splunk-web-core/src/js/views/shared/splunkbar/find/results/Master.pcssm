

/*  Find Results */
.view {
    composes: view from '../../../MenuDialog.pcssm';
    background-color: $white;
    width: 360px;
}

.list {
    composes: menu from '../../../MenuDialog.pcssm';
    li {
        > a {
            white-space: nowrap;
            overflow: hidden;
            text-overflow: ellipsis;
        }
    }

}

.arrow {
    composes: arrow from '../../../MenuDialog.pcssm';
}

.searchLink {
    border-top: 0;
}

.primaryLink {
    composes: primaryLink from '../../../MenuDialog.pcssm';
    composes: searchLink;
}
.secondaryLink {
    composes: secondaryLink from '../../../MenuDialog.pcssm';
    composes: searchLink;
}

.noResults {
    text-align: center;
    padding-top: 10px;
    padding-bottom: 10px;
    color:$textGray;
    border-bottom: 1px solid $borderColor;
}

.sharedSplunkbarFindFindprogress {
    background-color: $white;
    width: 360px;
}
